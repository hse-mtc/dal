    "normalize.css": "7.0.0",
    "nprogress": "0.2.0",
    "path-to-regexp": "2.4.0",
    "vue": "2.6.11",
    "vue-functional-calendar": "^2.6.7",
    "vue-lodash": "^2.1.2",
